using System;
using System.Linq;

class Program
{
    static void Main(string[] args)
    {
        int[] numbers = { 3, 10, 7, 8, 4, 5, 9 };

        var result = numbers
            .Where(n => n % 2 == 1) // Відбираємо тільки непарні числа
            .Select(n => n.ToString()) // Конвертуємо кожне число в його строкове представлення
            .OrderBy(s => s); // Сортуємо у лексикографічному порядку по зростанню

        foreach (var item in result)
        {
            Console.WriteLine(item);
        }
    }
}
